parameters:
    bootstrap: %currentWorkingDirectory%/vendor/autoload.php
    autoload_directories:
        - %currentWorkingDirectory%/src
    excludes_analyse:
        - %currentWorkingDirectory%/vendor
        - %currentWorkingDirectory%/etc
    ignoreErrors:
        - '#no value type specified in iterable#'
        - '#Property Randock\\DddPaginator\\Repository\\PaginatorRepository::\$repository with generic class Doctrine\\ORM\\EntityRepository does not specify its types: T#'